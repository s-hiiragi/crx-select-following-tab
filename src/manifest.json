{
    "manifest_version": 3,
    "name": "Select Following Tab",
    "version": "1.0",
    "host_permissions": [
        "https://twitter.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/",
                "https://twitter.com/home"
            ],
            "js": ["content_script.js"],
            "run_at": "document_end"
        }
    ]
}
